{
    "name": "andysamuelag/laravel-mongodb-passport",
    "description": "A package to allow laravel/passport use with jenssegers/laravel-mongodb",
    "homepage": "https://github.com/AndySamuelAG/laravel-mongodb-passport",
    "license": "MIT",
    "keywords": [
        "laravel",
        "laravel-mongodb",
        "laravel-mongodb-passport",
        "laravel-passport",
        "mongodb",
        "passport"
    ],
    "require": {
        "php": "^8.0",
        "jenssegers/mongodb": "^3.0",
        "laravel/passport": "6.0.* || 7.0.* || 7.4.* || 7.5.* || ^8.0 || ^9.0 || ^10 || ^11"
    },
    "autoload": {
        "psr-4": {
            "Sysvale\\Mongodb\\": "src"
        }
    },
    "authors": [
        {
            "name": "DesignMyNight",
            "email": "support@designmynight.com",
            "role": "Support"
        },
        {
            "name": "Andy",
            "email": "andyalvarado1997@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Sysvale\\Mongodb\\MongodbPassportServiceProvider"
            ]
        }
    }
}
